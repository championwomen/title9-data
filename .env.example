# Lightdash Local Development Environment Variables
# Copy this file to .env and fill in your actual values

# AWS/S3 Credentials (required for Lightdash query results)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
S3_BUCKET=your-lightdash-dev-bucket
S3_REGION=us-east-1

# Optional: For non-AWS S3-compatible storage (MinIO, etc.)
# S3_ENDPOINT=http://localhost:9000

# Lightdash Configuration
LIGHTDASH_SECRET=dev-secret-key-change-in-production
